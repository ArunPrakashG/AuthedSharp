<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
		<Nullable>enable</Nullable>
		<Version>1.2.0</Version>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>Arun Prakash</Authors>
		<Description>.NET Standard wrapper for beta.authed.io API</Description>
		<PackageProjectUrl>https://github.com/ArunPrakashG/AuthedSharp</PackageProjectUrl>
		<RepositoryUrl>https://github.com/ArunPrakashG/AuthedSharp</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryType>GitHub</RepositoryType>
		<PackageLicenseExpression></PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
